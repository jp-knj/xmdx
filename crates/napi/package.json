{
  "name": "@xmdx/napi",
  "version": "0.0.4",
  "description": "High-performance streaming Markdown to HTML parser built with Rust for Astro (Node.js NAPI bindings)",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "xmdx",
    "package": "@xmdx/napi",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish -t npm",
    "build": "napi build --platform --release --esm",
    "build:napi": "pnpm run build",
    "smoke:napi": "node ../../scripts/smoke-napi.mjs",
    "test": "bun test"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.4.1",
    "prettier": "^3.6.2"
  },
  "keywords": [
    "markdown",
    "parser",
    "rust",
    "napi",
    "streaming",
    "performance",
    "html"
  ],
  "author": "jp-knj",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jp-knj/xmdx"
  },
  "engines": {
    "node": ">= 10"
  },
  "optionalDependencies": {
    "@xmdx/napi-darwin-x64": "0.0.4",
    "@xmdx/napi-darwin-arm64": "0.0.4",
    "@xmdx/napi-win32-x64-msvc": "0.0.4",
    "@xmdx/napi-linux-x64-gnu": "0.0.4",
    "@xmdx/napi-linux-arm64-gnu": "0.0.4",
    "@xmdx/napi-linux-arm64-musl": "0.0.4",
    "@xmdx/napi-linux-x64-musl": "0.0.4"
  }
}
